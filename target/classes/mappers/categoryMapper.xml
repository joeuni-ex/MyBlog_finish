<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="category">

<!-- 전체 카테고리 리스트 출력  -->
<select id="categoryList" resultType="com.demo.domain.CategoryInfoVO">
	SELECT info_idx,category_name
	FROM category_info
	ORDER BY info_idx
</select>



</mapper>
